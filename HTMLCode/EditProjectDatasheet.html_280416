<script type="text/javascript">
    $(function () {
        $("#accordion").accordion({
            header: "h3",
            collapsible: true,
            heightStyle: "content",
            navigation: false
        });
        
    
        //to below script is used to get focused in the control on according click        
	$("h3.ui-accordion-header").click(function(){
		var isControlFocused=false;//This variable is used to stop iteration of loop once control is focused.
			
			$(this).nextAll('div').first().find('div').each(function(){
				 	$(this).each(function()
								{
										if(!isControlFocused)
										{
											$(this).find('input,select,textarea').each(function()
													{	
														if(typeof($(this).attr('type'))!=='undefined' && $(this).attr('readonly')!='readonly')
															{
																if($(this).val()=='Get User' && $(this).attr('type')=='button')
																{
																	$(this).focus();
																	isControlFocused=true;															
																	return false;
																}
																else if( $(this).attr('type')!='button')
																{
																	$(this).focus();
																	isControlFocused=true;															
																	return false;
																}

			
															}
														else if( ($(this)[0].tagName=='select'||$(this)[0].tagName=='TEXTAREA' )&& $(this).attr('disabled')!='disabled')
														{
																$(this).focus();
																isControlFocused=true;
																return false;
			
														}		
													});	
												return false;								
											}	
									});
							});
								
				});       
		 $('#accordion button').click(function (e) {
            e.preventDefault();
            var delta = ($(this).is('.next') ? 1 : -1);

            if (delta == 1) {
                switch ($('#accordion').accordion('option', 'active') + delta) {
                    case 1:
                        if (ValidateAuthorSection())
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        break;
                    case 2:
                        if (ValidateProjectInfo()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                    case 3:
                        if (ValidateValueDetails()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                    case 4:
                        if (ValidateProjectDesc()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                    case 5:
                        if (ValidateTeamDetails()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                    case 6:
                        if (ValidateProjectData()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                    case 7:
                        if (ValidateTestimonials()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                    case 8:
                        if (ValidateAwards()) {
                            $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        }
                        break;
                }
            }
            else {
                $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
            }

        });
    });
</script>

<script type="text/javascript">
    window.alert = function (message) {
        $(document.createElement('div'))
            .attr({ 'class': 'alert' })
            .html(message)
            .dialog({
                buttons: {
                    OK: function () {
                        $(this).dialog('close');
                        $(".v4master").css("overflow-y", "auto");
                    }
                },
                close: function () {
                    $(this).remove();
                    $(".v4master").css("overflow-y", "auto");
                },
                draggable: false,
                modal: true,
                resizable: false,
                width: 'auto',

            });

        $(".alert").parent().addClass("dummyclass");
        $(".dummyclass").addClass("popupfixing");

    };
    window.confirm = function (message) {
        $(document.createElement('div'))
            .attr({ 'class': 'confirm' })
            .html(message)
            .dialog({
                resizable: false,
                width: 'auto',
                modal: true,
                buttons: {
                    Yes: function () {
                        window.location.replace("/Lists/Project%20Evidence%20Bites/NewForm.aspx?PDID=" + projectDatasheetID);
                    },
                    No: function () {
                        $(this).dialog("close");
                        window.location.replace("/Pages/Add-Data.aspx");
                    }
                }
            });

        $(".confirm").parent().addClass("dummyclass");
        $(".dummyclass").addClass("popupfixing");
    };
</script>

