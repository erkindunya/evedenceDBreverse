<script type="text/javascript">
    $(function () {
        $("#accordion").accordion({
            header: "h3",
            collapsible: true,
            heightStyle: "content",
            navigation: false
        });
        $('#accordion button').click(function (e) {
            e.preventDefault();
            var delta = ($(this).is('.next') ? 1 : -1);

            if (delta == 1) {
                switch ($('#accordion').accordion('option', 'active') + delta) {
                    case 1:
                    case 2:
                    case 3:
                    case 4:
                    case 5:
                    case 6:
                    case 7:
                    case 8:
                        $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
                        break;
                }
            }
            else {
                $('#accordion').accordion('option', 'active', ($('#accordion').accordion('option', 'active') + delta));
            }

        });
    });
</script>

<script type="text/javascript">
    window.alert = function (message) {
        $(document.createElement('div'))
            .attr({ 'class': 'alert' })
            .html(message)
            .dialog({
                buttons: {
                    OK: function () {
                        $(this).dialog('close');
                        $(".v4master").css("overflow-y", "auto");
                        window.location.replace("/Pages/Add-Data.aspx");
                    }
                },
                close: function () {
                    $(this).remove();
                    $(".v4master").css("overflow-y", "auto");
                    window.location.replace("/Pages/Add-Data.aspx");
                },
                draggable: false,
                modal: true,
                resizable: false,
                width: 'auto',

            });

        $(".alert").parent().addClass("dummyclass");
        $(".dummyclass").addClass("popupfixing");

    };
    window.confirm = function (message) {
        $(document.createElement('div'))
            .attr({ 'class': 'confirm' })
            .html(message)
            .dialog({
                resizable: false,
                width: 'auto',
                modal: true,
                buttons: {
                    OK: function () {
                        window.location.replace("/Lists/Project%20Evidence%20Bites/NewForm.aspx?Source=/Pages/Add-Data.aspx&PDID=" + projectDatasheetID);
                    },
                    Cancel: function () {
                        $(this).dialog("close");
                    }
                }
            });

        $(".confirm").parent().addClass("dummyclass");
        $(".dummyclass").addClass("popupfixing");
    };
</script>

